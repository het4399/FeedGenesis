# Frontend Environment Variables

# Backend API URL
VITE_BACKEND_URL=http://localhost:3001

# Supabase Configuration (for auth and storage only)
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# YouTube OAuth Configuration
VITE_YOUTUBE_CLIENT_ID=your_youtube_client_id_here

# Facebook/Instagram OAuth Configuration
VITE_FACEBOOK_CLIENT_ID=your_facebook_app_id_here

# Feature Flags
VITE_ENABLE_AI_GENERATION=true
VITE_ENABLE_VIDEO_TEMPLATES=true
VITE_ENABLE_SOCIAL_INTEGRATION=true

# Development
VITE_DEV_MODE=true 